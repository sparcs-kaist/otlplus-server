{
  "name": "@otl/api-interface",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "pnpm build",
    "dev": "node_modules/.bin/tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "mock:front": "node_modules/.bin/tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "mock:back": "node_modules/.bin/tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "typecheck": "node_modules/.bin/tsc",
    "build": "node_modules/.bin/tsc",
    "api-docs:build": "ts-node src/docs/server-nest/docs-generator.ts",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./src/docs": "./dist/src/docs/index.js",
    "./src/docs/server-nest": "./dist/src/docs/server-nest/generated/index.js",
    "./src/entities": "./dist/src/entities/index.js",
    "./src/interfaces": "./dist/src/interfaces/index.js",
    "./src": "./dist/src/index.js"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.2",
    "ts-morph": "^23.0.0",
    "@prisma/client": "^6.1.0",
    "cron-validator": "^1.3.1",
    "prisma": "^6.1.0"
  },
  "devDependencies": {
    "prisma": "^6.1.0"
  }
}
